 font-weight: 500;\r\n  margin: 30px 0px;\r\n  text-shadow: 2px 5px 5px rgba(0, 0, 0, 0.5);\r\n}\r\n\r\n.current .temp span {\r\n  font-weight: 500;\r\n}\r\n\r\n.current .weather {\r\n  color: white;\r\n  font-size: 32px;\r\n  font-weight: 700;\r\n  font-style: italic;\r\n  margin-bottom: 15px;\r\n  text-shadow: 0px 3px rgba(0, 0, 0, 0.4);\r\n}\r\n\r\n.current .hi-low {\r\n  color: white;\r\n  font-size: 24px;\r\n  font-weight: 500;\r\n  text-shadow: 0px 3px rgba(0, 0, 0, 0.4);\r\n}\r\n\r\n.current .hi-low-text {\r\n  color: #dbe3e5;\r\n  font-size: 16px;\r\n  font-weight: 300;\r\n  text-shadow: 0px 3px rgba(0, 0, 0, 0.4);\r\n}\r\n\r\n/*cards*/\r\n.container {\r\n  display: grid;\r\n  grid-template-columns: 1fr 1fr 1fr 1fr;\r\n  grid-gap: 10px;\r\n  margin: 10px;\r\n  justify-items: center;\r\n  align-items: center;\r\n}\r\n\r\n.card {\r\n  background-image: linear-gradient(\r\n    to bottom,\r\n    rgba(17, 29, 94, 0.3),\r\n    rgba(17, 29, 94, 0.6)\r\n  );\r\n  color: white;\r\n  border-radius: 12px;\r\n  text-align: center;\r\n  align-items: center;\r\n  padding: 15px 15px 15px;\r\n}\r\n\r\n.card .date2,\r\n.date3,\r\n.date4,\r\n.date5 {\r\n  color: white;\r\n  font-size: 16px;\r\n}\r\n\r\n.card .temp2,\r\n.temp3,\r\n.temp4,\r\n.temp5 {\r\n  color: white;\r\n  font-size: 34px;\r\n  font-weight: 900;\r\n  margin: 20px 0px;\r\n  text-shadow: 2px 5px rgba(0, 0, 0, 0.5);\r\n}\r\n\r\n.card .weather2,\r\n.weather3,\r\n.weather4,\r\n.weather5 {\r\n  color: white;\r\n  font-size: 20px;\r\n  font-weight: 700;\r\n  font-style: italic;\r\n  margin-bottom: 15px;\r\n  text-shadow: 0px 3px rgba(0, 0, 0, 0.4);\r\n}\r\n.marquee {\r\n  width: 350px;\r\n  margin: 0 auto;\r\n  background: rgb(112, 140, 201);\r\n  white-space: nowrap;\r\n  overflow: hidden;\r\n  box-sizing: border-box;\r\n  color: rgb(6, 6, 36);\r\n  font-size: 18px;\r\n}\r\n\r\n.marquee span {\r\n  display: inline-block;\r\n  padding-left: 100%;\r\n  text-indent: 0;\r\n  animation: marquee 15s linear infinite;\r\n}\r\n\r\n.marquee span:hover {\r\n  animation-play-state: paused;\r\n}\r\n\r\n@keyframes marquee {\r\n  0% {\r\n    transform: translate(0, 0);\r\n  }\r\n  100% {\r\n    transform: translate(-100%, 0);\r\n  }\r\n}\r\n\r\n/* SMALL SCREEN */\r\n@media screen and (max-width: 1024px) {\r\n  .container {\r\n    grid-template-columns: 1fr;\r\n  }\r\n}\r\n"},"lineCount":null}},"error":null,"hash":"2bd61e5d13b312db2ca623f811657393","cacheData":{"env":{}}}